{
  "name": "quill-attachments",
  "description": "Attachments plugin for Quill rich text editor",
  "author": "drake.aren@pm.me",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "watch": "parcel watch --dist-dir ./demo/dist ./demo/index.html",
    "build": "parcel build --dist-dir ./dist ./src/index.js",
    "demo": "serve ./demo/dist",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not dead"
  ],
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@commitlint/cz-commitlint": "^13.1.0",
    "commitizen": "^4.2.4",
    "husky": "^7.0.2",
    "parcel": "^2.0.0-rc.0",
    "serve": "^12.0.1"
  },
  "dependencies": {
    "quill": "^1.3.7"
  }
}
